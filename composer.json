        "web-token/jwt-core": "^3.0",
        "web-token/jwt-signature-algorithm-rsa": "^3.0",
        "jms/serializer": "^3.8",
        "symfony/config": "^v4.4.13|^5.1.5",
        "symfony/yaml": "^v4.4.13|^5.1.5",
        "symfony/expression-language": "^v4.4.13|^5.1.5",
        "web-token/jwt-checker": "^3.0"
    },
    "autoload": {
        "psr-4": {
