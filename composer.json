{
    "name": "halloverden/symfony-security",
    "description": "Compliments Symfony Security with Access Definitions and utils",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Roar Furunes",
            "email": "tom.roar@halloverden.no"
        },
        {
            "name": "Erich Json Fosse",
            "email": "erich@halloverden.no"
        }
