    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "symfony/security-core": "^v4.4.13|^5.1.5",
        "jms/metadata": "^2.1",
        "doctrine/annotations": "^1.8",
        "doctrine/collections": "^1.6",
        "symfony/security-guard": "^v4.4.13|^5.1.5",
        "halloverden/symfony-http-exceptions": "^1.0.1",
