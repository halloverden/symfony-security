{
    "name": "halloverden/symfony-security",
    "description": "Compliments Symfony Security",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Roar Furunes",
            "email": "tom.roar@halloverden.no"
        }
    ],
    "keywords": [
        "access definition",
        "authenticator",
        "security",
        "symfony",
        "voter"
    ],
    "support": {
        "issues": "https://github.com/halloverden/symfony-security/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "symfony/security-core": "^4.4|^5.0",
        "jms/metadata": "^2.1",
        "doctrine/annotations": "^1.8",
        "doctrine/collections": "^1.6",
        "symfony/security-guard": "^4.4|^5.0",
        "halloverden/symfony-http-exceptions": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "HalloVerden\\Security\\": "src"
        }
    },
    "require-dev": {
        "symfony/validator": "^4.4|^5.0"
    }
}
